---
import Gallery from "../../components/Gallery.astro";
import BaseLayout from "../../layouts/BaseLayout.astro";
const photos = await Astro.glob("../../assets/kyoto-2023/*.jpg");
---

<BaseLayout sideBarActiveItemID="kyoto-2023">
  <div class="pb-12 mt-5">
    <div class="text-3xl font-bold mb-10">⛩️ Kyoto</div>
    <Gallery photos={photos} />
  </div>
</BaseLayout>
